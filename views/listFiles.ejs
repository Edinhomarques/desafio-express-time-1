<%- include('./partials/header') %>

<main class=" text-center">
  <h1 class="h3">Inscritos na newsletter</h1>
  <div class="">
  <table class="table table-bordered">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">name</th>
        <th scope="col">email</th>
        <th scope="col">data</th>
        <th scope="col">hora</th>
      </tr>
    </thead>
    <tbody>
      <% listaNew.map((item, index) => {%>
      <tr>
        <th scope="row"><%= index + 1%></th>
        <td><%= item.nome %></td>
        <td><%= item.email %></td>
        <td><%= item.data %></td>
        <td><%= item.horario %></td>
        
      </tr>
      <%})%>
    </tbody>
  </table>
  </div>
